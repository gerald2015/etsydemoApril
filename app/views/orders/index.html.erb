<h1>Listing orders </h1>
<table>
  <thead>
    <tr>
      <th>Address</th>
      <th>City</th>
      <th>State</th>
    </tr>
  </thead>
  <tbody>
    <% @orders.each do |order| %>
    <tr>
      <td><%= order.address %></td>
      <td><%= order.city %></td>
      <td><%= order.state %></td>
    </tr>
    <% end %>
  </tbody>
</table>
<br>
<%= link_to 'New Order', new_listing_order_path %>